<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Model.GuestBook,Model" table="GuestBook" lazy="true">
    <id name="Id" column="ID" type="int">
      <generator class="native" />
    </id>
    <property type="int" name="Caseid" column="[Caseid]" />
    <property type="string" length="50" name="Name" column="[Name]" />
    <property type="string" length="50" name="Tel" column="[Tel]" />
    <property type="string" length="100" name="Email" column="[eMail]" />
    <property type="string" length="50" name="Qq" column="[QQ]" />
    <property type="string" length="50" name="CodeNum" column="[CodeNum]" />
    <property type="string" length="100" name="Address" column="[Address]" />
    <property type="string" length="100" name="Title" column="[Title]" />
    <property type="string" length="1000" name="Remark" column="[Remark]" />
    <property type="int" name="Sequence" column="[Sequence]" />
    <property type="DateTime" name="PostDate" column="[PostDate]" />
    <property type="string" length="50" name="Postip" column="[PostIP]" />
    <property type="string" length="1000" name="Reply" column="[Reply]" />
    <property type="DateTime" name="ReplyDate" column="[ReplyDate]" />
    <property type="string" length="50" name="Replyip" column="[ReplyIP]" />
    <property type="Boolean" name="IsShow" column="[IsShow]" />
  </class>
</hibernate-mapping>
<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Model.FileList,Model" table="FileList" lazy="true">
    <id name="FileId" column="FileId" type="int">
      <generator class="native" />
    </id>
    <property type="int" name="UserId" column="[UserId]" />
    <property type="int" name="CaseId" column="[CaseId]" />
    <property type="int" name="Parentid" column="[Parentid]" />
    <property type="int" name="Pindex" column="[Pindex]" />
    <property type="int" name="ProjectId" column="[ProjectId]" />
    <property type="string" length="250" name="Path" column="[Path]" />
    <property type="string" length="250" name="Title" column="[Title]" />
    <property type="string" length="10" name="Ext" column="[Ext]" />
    <property type="double" name="Filesize" column="[Filesize]" />
    <property type="string" length="250" name="Content" column="[Content]" />
    <property type="string" length="250" name="IndexImagePath" column="[IndexImagePath]" />
    <property type="DateTime" name="Pubdate" column="[Pubdate]" />
  </class>
</hibernate-mapping>